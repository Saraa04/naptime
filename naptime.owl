<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/39328/ontologies/2020/11/naptime#"
     xml:base="http://www.semanticweb.org/39328/ontologies/2020/11/naptime"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:naptime="http://www.semanticweb.org/39328/ontologies/2020/11/naptime#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/39328/ontologies/2020/11/naptime">
        <owl:versionIRI rdf:resource="http://www.semanticweb.org/39328/ontologies/2020/11/naptime"/>
        <dc:creator xml:lang="en">Elena Alloca</dc:creator>
        <dc:creator xml:lang="en">Katarina Lucic</dc:creator>
        <dc:creator xml:lang="en">Sara Kitanoska</dc:creator>
        <terms:title xml:lang="en">NapTime ontology</terms:title>
        <Rights xml:lang="en">This work is distrubuted under a Creative Commons Attribution License (http://creativecommons.org/licenses/by/3.0/).</Rights>
        <rdfs:comment xml:lang="en">Here we need to insert the one phrase description of our project.</rdfs:comment>
        <owl:versionInfo xml:lang="en">1.0.0</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
